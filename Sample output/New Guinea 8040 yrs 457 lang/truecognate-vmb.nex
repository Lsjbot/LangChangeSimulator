END;

BEGIN PAUP;
	outgroup protovmb4;
	set criterion=parsimony;
	hsearch nreps=25;
	describe 1 / plot=phylogram;
	savetrees file=G:\Ling\LangChangeSimulator\output\New Guinea 8040 yrs 2023-09-22\truecognate-vmb.tre brlens;
	contree /treeFile=G:\Ling\LangChangeSimulator\output\New Guinea 8040 yrs 2023-09-22\truecognate-vmb-consensus.nex;
END;
